{
  "name": "myapp",
  "version": "0.0.0",
  "size-limit": [
    {
      "webpack": false,
      "path": "../Ecom.Rpat.Web/wwwroot/*.js"
    }
  ],
  "scripts": {
    "swagger": "nswag run p4.nswag /runtime:NetCore31",
    "ng": "ng",
    "w": "ng build --watch",
    "start": "ng serve --open --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer ../Ecom.Rpat.Web/wwwroot/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "fmt": "tsfmt -r",
    "imports": "organize-imports-cli tsconfig.json",
    "up": "ncu -u -x bootstrap,core-js,typescript,@types/bootstrap,webpack",
    "e2e": "ng e2e",
    "nghm": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng",
    "build:prod_ci": "npm run nghm -- build --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.2",
    "@angular/common": "15.0.2",
    "@angular/compiler": "15.0.2",
    "@angular/core": "15.0.2",
    "@angular/forms": "15.0.2",
    "@angular/platform-browser": "15.0.2",
    "@angular/platform-browser-dynamic": "15.0.2",
    "@angular/router": "15.0.2",
    "@types/file-saver": "^2.0.2",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "3.4.1",
    "core-js": "^2.6.5",
    "css-loader": "^6.7.2",
    "file-loader": "^6.2.0",
    "file-saver": "^2.0.5",
    "guid-typescript": "^1.0.9",
    "ie-shim": "^0.1.0",
    "jquery": "^3.6.0",
    "masonry-layout": "^4.2.2",
    "ngx-bootstrap": "6.2.0",
    "ngx-dnd": "^2.0.0",
    "ngx-simple-modal": "1.4.17",
    "npm-check-updates": "^11.5.1",
    "rxjs": "6.6.7",
    "style-loader": "^3.3.1",
    "toastr": "^2.1.4",
    "ts-debounce": "3.0.0",
    "ts-smart-logger": "^0.1.0",
    "url-loader": "^4.1.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.2",
    "@angular/cli": "15.0.2",
    "@angular/compiler-cli": "15.0.2",
    "@angular/language-service": "15.0.2",
    "@fortawesome/fontawesome-free": "5.15.3",
    "@types/bootstrap": "4.5.0",
    "@types/chosen-js": "^1.8.2",
    "@types/jasmine": "3.6.9",
    "@types/jasminewd2": "~2.0.8",
    "@types/jquery": "3.5.5",
    "@types/node": "14.14.41",
    "@types/toastr": "^2.1.38",
    "codelyzer": "6.0.1",
    "nswag": "13.10.9",
    "organize-imports-cli": "^0.8.0",
    "protractor": "~7.0.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.8.4",
    "typescript-formatter": "^7.2.2",
    "webpack": "5.0",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^5.0.0"
  }
}
